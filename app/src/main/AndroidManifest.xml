<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fhircondition">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FHIRCondition">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name = ".RegisterActivity"></activity>
        <activity android:name = ".anamnesisActivity"></activity>
        <activity android:name = ".registerDiagnosisActivity"></activity>
        <activity android:name = ".registerIllnessActivity"></activity>
        <activity android:name = ".ListAllPatients"></activity>
        <activity android:name = ".deletePatient"></activity>
    </application>

</manifest>